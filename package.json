{
  "name": "meshcore-monitor",
  "version": "1.0.1",
  "description": "Web application for monitoring Meshtastic and MeshCore mesh networks",
  "license": "BSD-3-Clause",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "tsx --watch src/server/server.ts",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "build": "tsc && vite build",
    "build:server": "tsc --project tsconfig.server.json && mkdir -p dist/server/routes/v1 && cp src/server/routes/v1/openapi.yaml dist/server/routes/v1/",
    "start": "node dist/server/server.js",
    "preview": "vite preview",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:quick-start": "tests/test-quick-start.sh",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "migrate-db": "tsx src/cli/migrate-db.ts"
  },
